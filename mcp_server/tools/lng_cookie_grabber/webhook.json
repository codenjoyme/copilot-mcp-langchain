{
  "name": "cookie-status-server",
  "port": 9000,
  "path": "/api",
  "bind_host": "localhost",
  "html_routes": [
    {
      "pattern": "/cookies/{sessionId}",
      "template": "mcp_server/tools/lng_cookie_grabber/status.html",
      "mapping": {
        "SESSION_ID": "{! url.sessionId !}",
        "PORTALS": "[\"https://google.com\", \"https://deepl.com\"]"
      }
    },
    {
      "pattern": "/download/background.js",
      "template": "mcp_server/tools/lng_cookie_grabber/templates/background.js",
      "mapping": {
        "WS_SERVER_URL": "{! 'http://localhost:9002' !}"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/javascript",
          "Content-Disposition": "attachment; filename=\"background.js\""
        }
      }
    },
    {
      "pattern": "/download/manifest.json",
      "template": "mcp_server/tools/lng_cookie_grabber/templates/manifest.json", 
      "mapping": {},
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json",
          "Content-Disposition": "attachment; filename=\"manifest.json\""
        }
      }
    }
  ],
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "body": {
      "message": "Cookie status server is running"
    }
  }
}